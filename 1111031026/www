#include <iostream>
#include<string>
using namespace std;

class dog
{
public:
void setdogname(string name)
{
    dogname=name;
}
string getdogname()const
{
    return dogname;
}
void setdogage(int age)
{
    dogage=age;
}
int getdogage()const
{
    return dogage;
}
void displayMessage()const
{
   cout<<"dogname "<<getdogname()<<" age is "<< getdogage() <<endl;
}
private:
 string dogname;
 int dogage;
};

int main() {
    string name;
    int age;
    dog Dooooog;

    cout << "dogge" << endl;
    cout << "enter name" << endl;
    getline(cin, name);
    Dooooog.setdogname(name);

    cout << "enter age" << endl;
    cin >> age;
    Dooooog.setdogage(age);
    if (age <= 20){ 
       Dooooog.displayMessage();
    }
    else {
        cout << "www" << endl;
    }
    return 0;
}
